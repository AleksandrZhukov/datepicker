{
  "compilerOptions": {
    "outDir": "dist",
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext", "DOM"],
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "rootDir": ".",
    "ignoreDeprecations": "5.0",
    "baseUrl": "./",
    "paths": {
      "renderer": ["./.jest/with-theme"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
